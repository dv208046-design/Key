<script>
// Bolinhas subindo
function criarBolinha() {
    const bolinha = document.createElement("div");
    bolinha.classList.add("bolinha");
    bolinha.style.left = Math.random() * 100 + "vw";
    const tamanho = Math.random() * 10 + 5;
    bolinha.style.width = tamanho + "px";
    bolinha.style.height = tamanho + "px";
    bolinha.style.animationDuration = (Math.random() * 4 + 3) + "s";
    document.body.appendChild(bolinha);
    setTimeout(()=>{bolinha.remove();},6000);
}
setInterval(criarBolinha,300);

// Primeira tela
const abrirBtn=document.getElementById("abrirBtn");
setTimeout(()=>{abrirBtn.classList.add("active"); abrirBtn.disabled=false;},5000);

abrirBtn.addEventListener("click",()=>{
    document.getElementById("primeira-tela").style.display="none";
    document.getElementById("segunda-tela").style.display="block";
});

// Passos
let passosCompletos=0;
const passos=document.querySelectorAll(".passo");

passos.forEach(passo=>{
    const btn=passo.querySelector(".iniciar-btn");
    const span=passo.querySelector(".temporizador");
    
    btn.addEventListener("click", ()=>{
        btn.disabled=true;
        btn.textContent="Aguardando aÃ§Ã£o...";
        setTimeout(()=>{
            let tempo=parseInt(passo.getAttribute("data-tempo"));
            span.textContent=tempo;

            const interval=setInterval(()=>{
                tempo--;
                span.textContent=tempo;
                if(tempo<=0){
                    clearInterval(interval);
                    span.textContent="âœ”";
                    passosCompletos++;
                    checarLiberar();
                }
            },1000);

        },2000);
    });
});

function checarLiberar(){
    if(passosCompletos===passos.length){
        document.getElementById("liberarBtn").style.display="inline-block";
    }
}

// ðŸ”¥ LISTA DAS SUAS KEYS FIXAS
const keysFixas = [
"FREE_gisi368heifbdk68",
"FREE_8Hd92jsla82ms9qp",
"FREE_a7Gs82ka9Bs72kd9",
"FREE_Ys82ms72js91Pqow",
"FREE_91jdla82msPq82la",
"FREE_w82ms7d9Qp28sHla",
"FREE_Po28s7hs9Qw8jdla",
"FREE_x82ms19Pq8sH72la"
];

// Liberar Key final
const liberarBtn=document.getElementById("liberarBtn");

liberarBtn.addEventListener("click", ()=>{
    liberarBtn.disabled=true;
    liberarBtn.textContent="Aguardando...";

    setTimeout(()=>{
        // ðŸ”¥ SORTEIA UMA DAS SUAS KEYS
        const key = keysFixas[Math.floor(Math.random() * keysFixas.length)];

        // Salva no div invisÃ­vel
        document.getElementById("key").textContent = key;

        // Mostra na tela
        document.getElementById("segunda-tela").style.display="none";
        document.getElementById("keyTela").style.display="block";
        document.getElementById("keyFinal").textContent=key;

    },4000);
});

// Copy Key
document.getElementById("copyBtn").addEventListener("click", ()=>{
    const key=document.getElementById("keyFinal").innerText;
    navigator.clipboard.writeText(key);
    alert("Key copiada!");
});
</script>

